{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "REDIS_CONNECTION_STRING": "redis://localhost:6379"
  },
  "DynamicBandwidthManagerConfiguration": {
    "Enabled": true,
    "TotalBytes": 10000,
    "PeriodInSec": 1,
    "ChunksConfiguration": {
      "Track": {
        "SizeInBytes": 2500,
        "RemainderPriority": 100
      },
      "Plot": {
        "SizeInBytes": 2500,
        "RemainderPriority": 90
      },
      "SensorStatus": {
        "SizeInBytes": 2500,
        "RemainderPriority": 70
      },
      "MissionStatus": {
        "SizeInBytes": 2500,
        "RemainderPriority": 80
      }
    }
  }
}
